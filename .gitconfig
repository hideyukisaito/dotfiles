[color]
        status = auto
        diff = auto
        interactive = auto
        grep = auto
        branch = auto
        log = auto
[core]
        autocrlf = input
[alias]
        ad = addd
        br = branch
        ba = branch -a
        bm = branch --merged
        bn = branch --no-mergedn
	cma = commit -a -m
        cam = commit -a --amend
        co = checkout
	ds = diff --stat --color
        df = diff
        d1 = diff HEAD~
        d2 = diff HEAD~~
        d3 = diff HEAD~~~
        d4 = diff HEAD~~~~
        d5 = diff HEAD~~~~~
        d6 = diff HEAD~~~~~~
        d7 = diff HEAD~~~~~~~
        d8 = diff HEAD~~~~~~~~
        d9 = diff HEAD~~~~~~~~~
        d10 = diff HEAD~~~~~~~~~~
        log-graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
        log-all = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
        ls = log --stat
        lp = log -p
        sh = show
        so = remote show origino
        st =status
